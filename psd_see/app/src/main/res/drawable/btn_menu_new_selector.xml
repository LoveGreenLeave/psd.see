<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
  android:constantSize="false" android:dither="false">
    <item android:state_pressed="true"
          android:drawable="@mipmap/menu_press_new" /> <!-- pressed -->
    <item android:state_enabled="false"
          android:drawable="@mipmap/menu_press_new"/>
    <item android:state_focused="true"
        android:state_pressed="@mipmap/menu_press_new"
        android:drawable="@mipmap/menu_normal_new" />
    <item android:drawable="@mipmap/menu_normal_new" />

</selector>
